                      regNo:
                        required: false
                      companyName:
                        required: false
                    status:
                      allowedValues:
                      - Current
                      - Former
                      - Active
                      - Inactive
                      required: false
                    address:
                      city:
                        required: false
                  - firstName:
                      required: false
                    lastName:
                      required: false
                    dateOfBirth:
                      required: false
                    company:
                      id:
                        required: true
                      regNo:
                        required: false
                      companyName:
                        required: false
                    status:
                      allowedValues:
                      - Current
                      - Former
                      - Active
                      - Inactive
                      required: false
                    address:
                      city:
                        required: false
                  - firstName:
                      required: false
                    lastName:
                      required: false
                    dateOfBirth:
                      required: false
                    company:
                      id:
                        required: false
                      regNo:
                        required: true
                      companyName:
                        required: false
                    status:
                      allowedValues:
                      - Current
                      - Former
                      - Active
                      - Inactive
                      required: false
                    address:
                      city:
                        required: false
                  - firstName:
                      required: false
                    lastName:
                      required: false
                    dateOfBirth:
                      required: false
                    company:
                      id:
                        required: false
                      regNo:
                        required: false
                      companyName:
                        required: true
                    status:
                      allowedValues:
                      - Current
                      - Former
                      - Active
                      - Inactive
                      required: false
                    address:
                      city:
